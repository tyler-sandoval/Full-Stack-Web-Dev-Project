@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Welcome to my Full Stack .Net Developer Project!</h1>
    <h4>This is my Learning Management System that is modeled after merchant services, something I am familiar with from my time working for Commerce Bank. Base functionality
    has been implemented. This project pushed me on my FED and Linq skills, and that's an aspect I really wanted to focus on to improve.</h4>
</div>

@section pageSpecificSlider {
<div class="carousel-inner" role="listbox">
    <div class="item active">
        <img class="img-responsive" src="~/Content/img/1920x1080/fsdp.png" alt="Slider Image">
        <div class="container">
            <div class="carousel-centered">
                <div class="margin-b-40">
                    <h1 class="carousel-title" style="text-shadow: 2px 2px 2px black, 0 0 25px purple, 0 0 10px indigo;">FSDP Merchant Services</h1>
                    <p>A Learning Management System Web Application <br /> For Merchant Services</p>
                </div>
            </div>
        </div>
    </div>
</div>
}